<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1" name="Kieker Scanner">
    <description>Provides a scanner for Kieker files.</description>
    <model>
        <class>my.project.plugin.scanner.model.MethodDescriptor</class>
        <class>my.project.plugin.scanner.model.AfterOperationEventDescriptor</class>
        <class>my.project.plugin.scanner.model.BeforeOperationEventDescriptor</class>
        <class>my.project.plugin.scanner.model.EventDescriptor</class>
        <class>my.project.plugin.scanner.model.TraceDescriptor</class>
        <class>my.project.plugin.scanner.model.TypeDescriptor</class>
        <class>my.project.plugin.scanner.model.RecordDescriptor</class>
    </model>
    <scanner>
        <class>my.project.plugin.scanner.KiekerFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>